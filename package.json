{
  "name": "chec-cli",
  "version": "0.0.0",
  "author": "Chec Platform",
  "bin": {
    "chec": "./bin/run"
  },
  "bugs": "https://github.com/chec/chec-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.2",
    "got": "^9.6.0",
    "inquirer": "^7.0.0",
    "lodash.get": "^4.4.2",
    "ora": "3.4.0",
    "query-string": "^6.8.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10",
    "mocha": "^5",
    "nock": "^11.4.0",
    "nyc": "^13",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/chec/chec-cli",
  "keywords": [
    "oclif"
  ],
  "license": "BSD-3-Clause",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "chec",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "chec/chec-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
